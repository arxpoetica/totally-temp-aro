{
  "database_url": "postgres://aro:aro@localhost/aro",
  "aro_service_url": "http://localhost:8080/aro-service",
  "client_schema": "client",
  "client_carrier_name": "VERIZON",
  "route_planning": ["fttp", "shortest_path"],
  "spend_by_city": false,
  "currency_symbol": "$",
  "ui": {
    "map_tools": {
      "locations": {
        "view": ["commercial", "residential"],
        "overlay": ["density"],
        "build": ["commercial", "residential", "combo"]
      },
      "boundaries": {
        "view": ["census_blocks", "county_subdivisions", "wirecenters"],
        "build": ["custom"]
      },
      "equipment": {
        "view": ["splice_point", "fiber_distribution_hub", "fiber_distribution_terminal", "existing_fiber"],
        "build": ["splice_point", "fiber_distribution_hub", "fiber_distribution_terminal"],
        "actions": ["recalculate", "clear"]
      },
      "competitors": {
        "view": ["all", "fiber_segments"],
        "overlay": ["density"]
      }
    },
    "top_bar_tools": ["market_profile", "customer_profile", "financial_profile"],
    "project_menu": ["share", "export_kml"],
    "icons": {
      "market_profile": "fa fa-briefcase"
    },
    "colors": {
      "fiber": "red",
      "fair_share": "blue"
    }
  }
}
