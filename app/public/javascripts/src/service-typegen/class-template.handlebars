{{importDependentClasses properties}}
class {{classNameExtractor id}} {

  // Constructor
  constructor() {
    {{#each properties}}
    {{memberDeclaration @key this.type this.id}}
    {{/each}}
  }

  // Returns a {{classNameExtractor id}} object from it's JSON representation
  static fromJSONObject(jsonObject) {
    var newObj = new {{classNameExtractor id}}()
    {{#each properties}}
    {{#if (isNotObject this.type)}}
    newObj.{{@key}} = jsonObject[{{@key}}]
    {{else}}
    newObj.{{@key}} = {{classNameExtractor this.id}}.fromJSON(jsonObject[{{@key}}])
    {{/if}}
    {{/each}}
    return newObj
  }
}

export default {{classNameExtractor id}}
