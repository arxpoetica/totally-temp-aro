  "description": "Network infrastructure planning application.",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test mocha test/models/*.js test/helpers/*.js --timeout 50000 --delay",
    "test-cov": "NODE_ENV=test istanbul --preserve-comments cover _mocha -- test/models/*.js test/helpers/*.js -R spec --timeout 50000 --delay",
    "test-ui": "./node_modules/.bin/karma start",
    "test-integration": "NODE_ENV=test protractor test/integration/conf.js",
    "test-all": "npm test && npm run test-ui && npm run test-integration",
    "test-wdio": "USER=dileep@metronconsulting.com PSWD=welcome01 ./node_modules/.bin/wdio wdio.conf.js",
    "test-unit-watch": "sleep 20 && ./node_modules/.bin/jest --watch",
    "build-service-types": "cd ./public/javascripts/src/service-typegen/src && node class-generator.js",
    "build-vendor-bundle": "chmod +x ./node_modules/uglify-js/bin/* && ./node_modules/browserify/bin/cmd.js -r rxjs -r ng-redux -p licensify | ./node_modules/uglify-js/bin/uglifyjs --comments > ./public/javascripts/vendors/vendor-bundle.min.js",
    "build-angularjs": "./node_modules/.bin/babel public/javascripts/src -d public/javascripts/lib --presets=@babel/preset-env --ignore public/javascripts/src/service-typegen/,public/javascripts/src/react/ && cd ./public/javascripts/src/components && find . -name \\*.html -exec cp --parents {} ../../lib/components/ \\;",
    "build": "npm run build-service-types && npm run build-vendor-bundle && npm run build-angularjs && webpack",
    "watch-client": "./node_modules/.bin/babel public/javascripts/src -d public/javascripts/lib --presets=@babel/preset-env --ignore public/javascripts/src/service-typegen/,public/javascripts/src/react/ --watch --copy-files --source-maps",
    "watch-server": "./node_modules/.bin/nodemon --inspect=0.0.0.0:9229 ./app.js",
    "dev": "npm run build-vendor-bundle && npm run build-service-types && ./node_modules/.bin/concurrently \"npm run watch-client\" \"npm run watch-server\" \"webpack --watch\" \"npm run test-unit-watch\"",
    "local": "./node_modules/.bin/concurrently \"npm run watch-client\" \"npm run watch-server\" \"webpack --watch\"",
    "lint": "standard \"public/javascripts/src/react/**/*.js\" \"public/javascripts/src/react/**/*.jsx\"",
    "lint-fix": "standard \"public/javascripts/src/react/**/*.js\" \"public/javascripts/src/react/**/*.jsx\" --fix"
  },
