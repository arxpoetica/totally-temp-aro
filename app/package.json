{
  "name": "aro",
  "version": "0.0.0",
  "description": "Network infrastructure planning application",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test mocha test/models/*.js test/helpers/*.js --timeout 50000 --delay",
    "test-cov": "NODE_ENV=test istanbul --preserve-comments cover _mocha -- test/models/*.js test/helpers/*.js -R spec --timeout 50000 --delay",
    "test-ui": "./node_modules/.bin/karma start",
    "test-integration": "NODE_ENV=test protractor test/integration/conf.js",
    "test-all": "npm test && npm run test-ui && npm run test-integration",
    "build-vendor-bundle": "chmod +x ./node_modules/uglify-js/bin/* && ./node_modules/browserify/bin/cmd.js -r vector-tile -r pbf -r rxjs -r point-in-polygon -p licensify | ./node_modules/uglify-js/bin/uglifyjs --comments > ./public/javascripts/vendors/vendor-bundle.min.js",
    "build-angularjs": "./node_modules/babel-cli/bin/babel.js public/javascripts/src -d public/javascripts/lib && cd ./public/javascripts/src/components && find . -name \\*.html -exec cp --parents {} ../../lib/components/ \\;",
    "build": "npm run build-vendor-bundle && npm run build-angularjs && npm run webpack-deploy",
    "watch-client": "./node_modules/babel-cli/bin/babel.js public/javascripts/src public/javascripts/src/components -d public/javascripts/lib --watch --copy-files --source-maps",
    "watch-server": "./node_modules/.bin/nodemon --inspect ./app.js",
    "webpack-deploy": "webpack",
    "webpack-dev": "webpack --watch",
    "dev": "./node_modules/.bin/concurrently \"npm run watch-client\" \"npm run watch-server\" \"npm run webpack-dev\"",
    "lint": "standard"
  },
  "author": "Altman Vilandrie &Co. Advanced Analytics and Products Division",
  "license": "BSD-2-Clause",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "standard": {
    "ignore": [
      "public/javascripts/lib",
      "public/javascripts/vendors",
      "test/integration",
      "test/ui/mocha.js",
      "test/ui/chai.js"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.2.22",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.3.13",
    "bcryptjs": "^2.2.1",
    "body-parser": "^1.13.2",
    "browserify": "^14.4.0",
    "busboy": "^0.2.13",
    "chai": "~3.0.0",
    "chai-string": "^1.1.6",
    "compression": "~1.5.0",
    "cookie-session": "^1.2.0",
    "csv-parse": "^1.1.3",
    "csv-stringify": "0.0.8",
    "dedent": "^0.6.0",
    "ejs": "^2.3.3",
    "express": "^4.13.1",
    "express-flash": "0.0.2",
    "gm": "^1.23.0",
    "json2csv": "^3.7.3",
    "licensify": "^3.1.3",
    "moment": "^2.10.6",
    "multer": "^1.2.0",
    "node-cache": "^3.0.0",
    "node-errors": "^0.2.0",
    "nodemailer": "^1.10.0",
    "nodemailer-ses-transport": "^1.3.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport-stub": "^1.0.1",
    "pbf": "^3.0.5",
    "pg": "^4.5.5",
    "pg-hstore": "^2.3.2",
    "pify": "^2.3.0",
    "point-in-polygon": "^1.0.1",
    "pync": "^1.0.0",
    "q": "^1.4.1",
    "request": "^2.75.0",
    "rxjs": "^5.4.1",
    "stream-transform": "^0.1.1",
    "supertest": "^1.1.0",
    "temp": "^0.8.3",
    "uglify-js": "^3.0.24",
    "underscore": "^1.8.3",
    "vector-tile": "^1.3.0",
    "webpack": "^3.0.0",
    "yargs": "^3.24.0"
  },
  "devDependencies": {
    "angular": "^1.4.3",
    "angular-mocks": "^1.4.3",
    "concurrently": "^3.5.1",
    "webpack": "^3.0.0",
    "hard-source-webpack-plugin": "^0.6.4",
    "istanbul": "^0.3.17",
    "karma": "^0.13.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.17.3",
    "phantomjs": "^1.9.17",
    "protractor": "^2.1.0",
    "standard": "^6.0.7",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "wdio-dot-reporter": "0.0.8",
    "wdio-firefox-profile-service": "0.0.2",
    "wdio-jasmine-framework": "^0.3.0",
    "wdio-junit-reporter": "^0.3.0",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.8",
    "webdriverio": "^4.6.2",
    "xml2js": "^0.4.10"
  }
}
